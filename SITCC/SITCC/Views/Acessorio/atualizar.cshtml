<h1>Atualizar </h1>

<form method="post" action="Atualizar">

    <div class="form-group">
        <label for="codigo">Código</label>
        <input type="text" class="form-control" id="codigo" value="@ViewBag.AcessorioAtualizar.proID" name="codigo">
    </div>


    <div class="form-group">
        <label for="nomeproduto">Nome</label>
        <input type="text" class="form-control" id="nomeproduto" value="@ViewBag.AcessorioAtualizar.nomeproduto" name="nomeproduto">
    </div>

    <div class="form-group">
        <label for="descricaoproduto">Descricao</label>
        <input type="text" class="form-control" id="descricaoproduto" value="@ViewBag.AcessorioAtualizar.descricaoproduto" name="descricaoproduto">
    </div>
    <div class="form-group">
        <label for="preco">Preço</label>
        <input type="text" class="form-control" id="preco" value="@ViewBag.AcessorioAtualizar.preco" name="preco">
    </div>
    <div class="form-group">
        <label for="fotoproduto">Foto</label>
        <input type="text" class="form-control" id="fotoproduto" value="@ViewBag.AcessorioAtualizar.fotoproduto" name="fotoproduto">
    </div>





    <button type="submit" class="btn btn-primary">Submit</button>
</form>